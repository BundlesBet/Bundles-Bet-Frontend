{
  "name": "bundles-frontend",
  "version": "0.1.0",
  "private": true,
  "author": "Bundles-dev-team",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "npx turbo type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "generate": "npx typechain --target=ethers-v5 './src/abis/*.json' --out-dir './src/types'"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.6",
    "@choc-ui/chakra-autocomplete": "^5.1.4",
    "@choc-ui/paginator": "^3.4.0",
    "@date-io/date-fns": "^2.16.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-table": "^7.7.14",
    "axios": "^1.2.4",
    "bn.js": "^5.2.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "ethers": "^5.0.0",
    "formik": "^2.2.9",
    "framer-motion": "^8.4.3",
    "next": "^13.1.1",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-table": "^7.8.0",
    "redux": "^4.2.0",
    "wagmi": "^0.9.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/cz-commitlint": "^17.4.1",
    "@playwright/test": "^1.29.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/react": "^18.0.26",
    "commitizen": "^4.2.6",
    "commitlint": "^17.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-sznm": "^1.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "next-sitemap": "^3.1.45",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "turbo": "^1.6.3",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  }
}
